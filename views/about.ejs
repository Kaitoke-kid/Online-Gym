<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Admin</title>
    <link rel="stylesheet" type="text/CSS" href="../style.css">
</head>
<body>
    
    <script src="admin.js"></script>
    <div class="image">
        <img src="../admin_gym.jfif" height="50" width="70" alt="Gym image">
    </div>
    <div class="connections">
        <h2>Prestigious Gym</h2>    
    </div>
    <div class="header">
        <a href="/fitness">Fitness Classes</a>
    </div>
    <div class="content">
        <% results.forEach(function(data) { %>
        <h4>Gym Address: <%= data.address %></h4>
        <h4>Phone number: <%= data.phone_number %></h4>
        <h4>Details: <%= data.details %></h4>
        <% }) %>
    </div>
    <div class="footer">
        <a href="/about">About</a>
        <a href="/contact">Contact</a>
    </div>
</body>
</html>